<% if (item.tags && item.tags.length){ %>
  <%
  var tags = [];
  item.tags.forEach(function(tag){
    tags.push('<a class="btn btn-primary btn-xs" href="' + config.root + tag.path + '">' + tag.name + '</a>');
  });
  %>
  <div class="tags">
    <span>Tagged with: </span><%- tags.join(' ') %>
  </div>
<% } %>